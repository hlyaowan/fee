<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aoyetech.fee.dal.dao.rechage.RechargeDAO">

	

	<!-- select 基本语句 -->
	<select id="Recharge_select" parameterType="RechargeDO"
		resultType="RechargeDO">
		select id as id,
		recharge_name as rechargeName,
		recharge_number as
		rechargeNumber,
		description as description,
		create_time as createTime,
		update_time as updateTime,
		extendation as
		extendation
		from fee_recharge
		<if test="start != null and count != null">
			limit #{start}, #{count}
		</if>
	</select>
	
	<!-- select 基本语句 -->
	<select id="Recharge_select_entity" parameterType="feeRechargeDO"
		resultType="RechargeDO">
		select id as id,
		recharge_name as rechargeName,
		recharge_number as
		rechargeNumber,
		description as description,
		create_time as createTime,
		update_time as updateTime,
		extendation as
		extendation
		from fee_recharge WHERE id = #{id}
	</select>

	<!-- select count() 基本语句 -->
	<select id="Recharge_count" parameterType="RechargeDO"
		resultType="java.lang.Integer">
		select count(1) from fee_recharge
	</select>

	<!-- insert into 基本语句 -->
	<insert id="Recharge_insert" parameterType="RechargeDO">
		insert into
		fee_recharge(recharge_name,recharge_number,description,create_time,update_time,extendation)
		values(#rechargeName#,#rechargeNumber#,#description#,#createTime#,#updateTime#,#extendation#)
	</insert>


	<!-- update 基本语句 -->
	<update id="Recharge_update" parameterType="RechargeDO">
		update
		fee_recharge
		set
		recharge_name=#rechargeName#,recharge_number=#rechargeNumber#,description=#description#,create_time=#createTime#,update_time=#updateTime#,extendation=#extendation#
		where id = #id#
	</update>

	<!-- delete 基本语句 -->
	<delete id="Recharge_delete" parameterType="RechargeDO">
		delete from
		fee_recharge
		where id = #id#
	</delete>
</mapper>