<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aoyetech.fee.dal.dao.rechargerecord.RechargeRecordDAO">
	
	<!-- select 基本语句 -->
	<select id="RechargeRecord_select" parameterType="RechargeRecordDO"
		resultType="RechargeRecordDO">
		select id as id,
		recharge_name as rechargeName,
		diamond_number as
		diamondNumber,
		decribe as decribe,
		create_time as
		createTime,
		update_time as updateTime,
		extendation as extendation
		from
		recharge_record WHERE 1 = 1
	</select>
	
	<!-- select 基本语句 -->
	<select id="RechargeRecord_select_entity" parameterType="RechargeRecordDO"
		resultType="RechargeRecordDO">
		select id as id,
		recharge_name as rechargeName,
		diamond_number as
		diamondNumber,
		decribe as decribe,
		create_time as
		createTime,
		update_time as updateTime,
		extendation as extendation
		from
		recharge_record WHERE id = #{id}
	</select>

	<!-- select count() 基本语句 -->
	<select id="RechargeRecord_count" parameterType="RechargeRecordDO"
		resultType="java.lang.Integer">
		select count(1) from recharge_record
		where 1 = 1
	</select>


	<!-- insert into 基本语句 -->
	<insert id="RechargeRecord_insert" parameterType="RechargeRecordDO">
		insert into
		recharge_record(recharge_name,diamond_number,decribe,create_time,update_time,extendation)
		values(#rechargeName#,#diamondNumber#,#decribe#,#createTime#,#updateTime#,#extendation#)
	</insert>

	<!-- update 基本语句 -->
	<update id="RechargeRecord_update" parameterType="RechargeRecordDO">
		update
		recharge_record
		set
		recharge_name=#rechargeName#,diamond_number=#diamondNumber#,decribe=#decribe#,create_time=#createTime#,update_time=#updateTime#,extendation=#extendation#
		where id = #id#
	</update>


	<!-- delete 基本语句 -->
	<delete id="RechargeRecord_delete" parameterType="RechargeRecordDO">
		delete from
		recharge_record
		where id = #id#
	</delete>
</mapper>